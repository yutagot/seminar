	langid = {english},
	file = {Tu et al. - 2017 - Enhancing the electrical conductivity of vacuum-ul.pdf:/Users/gotouyuuta/Zotero/storage/XM9X7MWA/Tu et al. - 2017 - Enhancing the electrical conductivity of vacuum-ul.pdf:application/pdf},
}

